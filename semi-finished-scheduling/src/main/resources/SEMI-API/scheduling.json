{
  "PUT": {
    "/scheduling": {
      "summary": "修改定时任务是否跟随项目启动",
      "params": {
        "@tb": "semi_scheduling",
        "id$$": "id",
        "enable$$": "enable",
        "cron$$": "cron",
        "args$$": "args",
        "mark$$": "mark"
      }
    }
  },
  "postQ": {
    "/run": {
      "summary": "运行定时任务",
      "params": {
        "@tb": "semi_scheduling",
        "@bean": "runCronTaskEnhance",
        "bean_name$$": "name"
      },
      "ruler": {
        "name": {
          "text": "name不能为空"
        }
      }
    },
    "cancel": {
      "summary": "停止运行任务",
      "params": {
        "@tb": "semi_scheduling",
        "@bean": "cancelCronTaskEnhance",
        "bean_name$$": "name"
      },
      "ruler": {
        "name": {
          "text": "name不能为空"
        },
        "interrupt": {
        }
      }
    }
  }
}